{
  "expo": {
    "name": "Fresh Meat",
    "description": "A tiny roller derby application for fresh meats",
    "slug": "roller-derby-fresh-meat",
    "privacy": "public",
    "githubUrl": "https://github.com/Zevran/roller-derby-fresh-meat",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.gaelreyrol.freshmeat",
      "supportsTablet": true
    },
    "android": {
      "package": "com.gaelreyrol.freshmeat",
      "permissions": []
    },
    "packagerOpts": {
      "assetExts": [
        "png"
      ]
    }
  }
}
